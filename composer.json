{
	"name": "boynoiz/liverpoolthailand",
	"description": "Base website for www.liverpoolthailand.com",
	"keywords": ["framework", "laravel", "cms"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.0.0",
		"laravel/framework": "5.3.*",
		"baum/baum": "~1.1",
		"kris/laravel-form-builder": "~1.7.0",
		"laracasts/flash": "~2.0",
		"cviebrock/eloquent-sluggable": "~4.0",
		"barryvdh/laravel-elfinder": "~0.3",
		"spatie/laravel-analytics": "~2.2",
		"caffeinated/menus": "~3.0",
		"yajra/laravel-datatables-oracle": "~6.0",
		"laravelcollective/html": "5.3.*",
		"mews/purifier": "~2.0",
		"spatie/laravel-permission": "^1.3",
		"jenssegers/agent": "^2.3",
		"jenssegers/date": "^3.1",
		"sammyk/laravel-facebook-sdk": "^3.0",
		"graham-campbell/exceptions": "^8.3",
		"barryvdh/elfinder-flysystem-driver": "^0.1.0",
		"guzzlehttp/guzzle": "^6.2",
		"predis/predis": "^1.0",
		"dingo/api": "1.0.*@dev",
		"tymon/jwt-auth": "0.5.*",
		"intervention/image": "^2.3",
		"league/glide-laravel": "^1.0",
		"pda/pheanstalk": "^3.1"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.5",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~5.0",
		"symfony/css-selector": "3.1.*",
		"symfony/dom-crawler": "3.1.*",
		"doctrine/dbal": "~2.5",
		"laracasts/generators": "~1.1",
		"barryvdh/laravel-ide-helper": "~2.0",
		"barryvdh/laravel-debugbar": "~2.0",
		"squizlabs/php_codesniffer": "~1.5",
		"filp/whoops": "^2.0",
		"phpspec/phpspec": "~2.4"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-install-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postInstall",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan optimize",
			"php artisan ide-helper:generate",
			"php artisan elfinder:publish"
		]
	},
	"config": {
		"preferred-install": "dist",
		"minimum-stability": "dev",
		"prefer-stable": true
	}
}
